	// Including header
	include blocks/header
	// Page content 
	.main-header.prodotti
		.container
			.main-caption
				h1
					|Prodotti
	section.content.products-content
		.container
			.row
				include blocks/products-filter-ibisqus	
				.col-md-9
					.row.filtri
						.col-sm-4.no-pad
							.filtri-img
								a(href="prodotti-export")
									img.img-responsive(src="/img/ibi-export-logo.jpg", alt="")
						.col-sm-4.no-pad
							.filtri-img.active
								a(href="prodotti-ibisqus")
									img.img-responsive(src="/img/ibisqus-ospedale-logo.jpg", alt="")
						.col-sm-4.no-pad
							.filtri-img
								a(href="prodotti-italia")
